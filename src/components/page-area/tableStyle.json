{
  "tableContainer": {
    "color": "white",
    "boxShadow": "0px 2px 1px -1px rgba(0,0,0,0.2), 0px 1px 1px 0px rgba(0,0,0,0.14), 0px 1px 3px 0px rgba(0,0,0,0.12)",
    "maxHeight": "calc(100vh - 250px)",
    "borderRadius": "5px"
  },
  "tableHeader": {
    "backgroundColor": "rgb(99,97,98)",
    "color": "#fff",
    "cursor": "pointer",
    "userSelect": "none"
  },
  "tableRow": {
    "&:nth-of-type(odd)": {
      "backgroundColor": "rgb(249,249,249)"
    },
    "&:nth-of-type(even)": {
      "backgroundColor": "rgb(238,181,104)"
    },
    "&:hover": {
      "backgroundColor": "#cacaca"
    }
  },
  "toolBar": {
    "direction": "ltr"
  },
  "spacer": {
    "flex": 0,
    "backgroundColor": "#000"
  },
  "wrapTable": {
    "flexGrow": 1
  },
  "success": {
    "backgroundColor": "#3d6f1c",
    "fontWeight": "600",
    "color": "#fff",
    "padding": "0.2em",
    "textTransform": "capitalize",
    "borderRadius": 5
  },
  "failed": {
    "backgroundColor": "#D61A3C",
    "fontWeight": "600",
    "color": "#fff",
    "padding": "0.2em",
    "textTransform": "capitalize",
    "borderRadius": 5
  }
}
